{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport axios from 'axios';\nexport default {\n  name: 'AddItem',\n  data() {\n    return {\n      itemName: '',\n      itemCategory: null,\n      itemDescription: '',\n      categories: [],\n      selectedFile: null,\n      imageUrl: '',\n      isEditMode: false,\n      itemId: null\n    };\n  },\n  methods: {\n    onFileChange(event) {\n      this.selectedFile = event.target.files[0];\n    },\n    async uploadFile() {\n      if (!this.selectedFile) return this.imageUrl;\n      const formData = new FormData();\n      formData.append('file', this.selectedFile);\n      try {\n        const response = await axios.post(`${process.env.VUE_APP_BACKEND_URL}/upload`, formData, {\n          headers: {\n            'Content-Type': 'multipart/form-data'\n          }\n        });\n        return response.data.filePath;\n      } catch (error) {\n        console.error('파일 업로드 실패:', error);\n        return '';\n      }\n    },\n    async addItem() {\n      if (!this.itemName || !this.itemCategory) {\n        alert('아이템 이름과 카테고리를 선택해주세요.');\n        return;\n      }\n      const imageUrl = await this.uploadFile();\n      const newItem = {\n        name: this.itemName,\n        categoryId: this.itemCategory,\n        imageUrl,\n        description: this.itemDescription\n      };\n      axios.post(`${process.env.VUE_APP_BACKEND_URL}/items`, newItem).then(() => {\n        alert('아이템이 성공적으로 추가되었습니다.');\n        this.resetForm();\n      }).catch(error => {\n        console.error('아이템 추가 실패:', error);\n      });\n    },\n    async updateItem() {\n      if (!this.itemName || !this.itemCategory) {\n        alert('아이템 이름과 카테고리를 선택해주세요.');\n        return;\n      }\n      const imageUrl = await this.uploadFile();\n      const updatedItem = {\n        name: this.itemName,\n        categoryId: this.itemCategory,\n        imageUrl,\n        description: this.itemDescription\n      };\n      axios.put(`${process.env.VUE_APP_BACKEND_URL}/items/${this.itemId}`, updatedItem).then(() => {\n        alert('아이템이 성공적으로 수정되었습니다.');\n        this.$router.push('/');\n      }).catch(error => {\n        console.error('아이템 수정 실패:', error);\n      });\n    },\n    fetchItemDetails() {\n      const itemId = this.$route.params.id;\n      if (itemId) {\n        this.isEditMode = true;\n        axios.get(`${process.env.VUE_APP_BACKEND_URL}/items/${itemId}`).then(response => {\n          const item = response.data;\n          this.itemId = item.id;\n          this.itemName = item.name;\n          this.itemCategory = item.category_id;\n          this.itemDescription = item.description;\n          this.imageUrl = item.image_url;\n        }).catch(error => {\n          console.error('Error fetching item details:', error);\n        });\n      }\n    },\n    fetchCategories() {\n      axios.get(`${process.env.VUE_APP_BACKEND_URL}/categories`).then(response => {\n        this.categories = response.data;\n      }).catch(error => {\n        console.error('Error fetching categories:', error);\n      });\n    },\n    resetForm() {\n      this.itemName = '';\n      this.itemCategory = null;\n      this.itemDescription = '';\n      this.selectedFile = null;\n      this.imageUrl = '';\n      this.isEditMode = false;\n      this.itemId = null;\n    }\n  },\n  created() {\n    this.fetchCategories();\n    this.fetchItemDetails();\n  }\n};","map":{"version":3,"names":["axios","name","data","itemName","itemCategory","itemDescription","categories","selectedFile","imageUrl","isEditMode","itemId","methods","onFileChange","event","target","files","uploadFile","formData","FormData","append","response","post","process","env","VUE_APP_BACKEND_URL","headers","filePath","error","console","addItem","alert","newItem","categoryId","description","then","resetForm","catch","updateItem","updatedItem","put","$router","push","fetchItemDetails","$route","params","id","get","item","category_id","image_url","fetchCategories","created"],"sources":["D:\\SecureCoding-Shopping-Site-A-MEN\\my-diary-app\\client\\src\\components\\AddItem.vue"],"sourcesContent":["<template>\r\n    <div class=\"add-item-page\">\r\n      <h2>{{ isEditMode ? '아이템 수정' : '새로운 아이템 추가' }}</h2>\r\n      <input type=\"text\" v-model=\"itemName\" placeholder=\"Item Name\" />\r\n      <select v-model=\"itemCategory\">\r\n        <option v-for=\"category in categories\" :key=\"category.id\" :value=\"category.id\">\r\n          {{ category.name }}\r\n        </option>\r\n      </select>\r\n      <textarea v-model=\"itemDescription\" placeholder=\"Item Description\"></textarea>\r\n      <input type=\"file\" @change=\"onFileChange\" />\r\n      <button @click=\"isEditMode ? updateItem() : addItem()\">{{ isEditMode ? '수정' : '추가' }}</button>\r\n    </div>\r\n  </template>\r\n  \r\n  <script>\r\n  import axios from 'axios';\r\n  \r\n  export default {\r\n    name: 'AddItem',\r\n    data() {\r\n      return {\r\n        itemName: '',\r\n        itemCategory: null,\r\n        itemDescription: '',\r\n        categories: [],\r\n        selectedFile: null,\r\n        imageUrl: '',\r\n        isEditMode: false,\r\n        itemId: null\r\n      };\r\n    },\r\n    methods: {\r\n      onFileChange(event) {\r\n        this.selectedFile = event.target.files[0];\r\n      },\r\n      async uploadFile() {\r\n        if (!this.selectedFile) return this.imageUrl;\r\n  \r\n        const formData = new FormData();\r\n        formData.append('file', this.selectedFile);\r\n  \r\n        try {\r\n          const response = await axios.post(`${process.env.VUE_APP_BACKEND_URL}/upload`, formData, {\r\n            headers: {\r\n              'Content-Type': 'multipart/form-data'\r\n            }\r\n          });\r\n          return response.data.filePath;\r\n        } catch (error) {\r\n          console.error('파일 업로드 실패:', error);\r\n          return '';\r\n        }\r\n      },\r\n      async addItem() {\r\n        if (!this.itemName || !this.itemCategory) {\r\n          alert('아이템 이름과 카테고리를 선택해주세요.');\r\n          return;\r\n        }\r\n  \r\n        const imageUrl = await this.uploadFile();\r\n  \r\n        const newItem = {\r\n          name: this.itemName,\r\n          categoryId: this.itemCategory,\r\n          imageUrl,\r\n          description: this.itemDescription\r\n        };\r\n  \r\n        axios.post(`${process.env.VUE_APP_BACKEND_URL}/items`, newItem)\r\n          .then(() => {\r\n            alert('아이템이 성공적으로 추가되었습니다.');\r\n            this.resetForm();\r\n          })\r\n          .catch(error => {\r\n            console.error('아이템 추가 실패:', error);\r\n          });\r\n      },\r\n      async updateItem() {\r\n        if (!this.itemName || !this.itemCategory) {\r\n          alert('아이템 이름과 카테고리를 선택해주세요.');\r\n          return;\r\n        }\r\n  \r\n        const imageUrl = await this.uploadFile();\r\n  \r\n        const updatedItem = {\r\n          name: this.itemName,\r\n          categoryId: this.itemCategory,\r\n          imageUrl,\r\n          description: this.itemDescription\r\n        };\r\n  \r\n        axios.put(`${process.env.VUE_APP_BACKEND_URL}/items/${this.itemId}`, updatedItem)\r\n          .then(() => {\r\n            alert('아이템이 성공적으로 수정되었습니다.');\r\n            this.$router.push('/');\r\n          })\r\n          .catch(error => {\r\n            console.error('아이템 수정 실패:', error);\r\n          });\r\n      },\r\n      fetchItemDetails() {\r\n        const itemId = this.$route.params.id;\r\n        if (itemId) {\r\n          this.isEditMode = true;\r\n          axios.get(`${process.env.VUE_APP_BACKEND_URL}/items/${itemId}`)\r\n            .then(response => {\r\n              const item = response.data;\r\n              this.itemId = item.id;\r\n              this.itemName = item.name;\r\n              this.itemCategory = item.category_id;\r\n              this.itemDescription = item.description;\r\n              this.imageUrl = item.image_url;\r\n            })\r\n            .catch(error => {\r\n              console.error('Error fetching item details:', error);\r\n            });\r\n        }\r\n      },\r\n      fetchCategories() {\r\n        axios.get(`${process.env.VUE_APP_BACKEND_URL}/categories`)\r\n          .then(response => {\r\n            this.categories = response.data;\r\n          })\r\n          .catch(error => {\r\n            console.error('Error fetching categories:', error);\r\n          });\r\n      },\r\n      resetForm() {\r\n        this.itemName = '';\r\n        this.itemCategory = null;\r\n        this.itemDescription = '';\r\n        this.selectedFile = null;\r\n        this.imageUrl = '';\r\n        this.isEditMode = false;\r\n        this.itemId = null;\r\n      }\r\n    },\r\n    created() {\r\n      this.fetchCategories();\r\n      this.fetchItemDetails();\r\n    }\r\n  };\r\n  </script>\r\n  \r\n  <style>\r\n  .add-item-page {\r\n    margin-top: 20px;\r\n    padding: 20px;\r\n    border: 1px solid #ddd;\r\n    display: flex;\r\n    flex-direction: column;\r\n    align-items: center;\r\n  }\r\n  .add-item-page input,\r\n  .add-item-page select,\r\n  .add-item-page textarea {\r\n    padding: 10px;\r\n    font-size: 16px;\r\n    margin-bottom: 10px;\r\n  }\r\n  .add-item-page button {\r\n    padding: 10px 20px;\r\n    font-size: 16px;\r\n    cursor: pointer;\r\n  }\r\n  </style>\r\n  "],"mappings":";AAgBE,OAAOA,KAAI,MAAO,OAAO;AAEzB,eAAe;EACbC,IAAI,EAAE,SAAS;EACfC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,QAAQ,EAAE,EAAE;MACZC,YAAY,EAAE,IAAI;MAClBC,eAAe,EAAE,EAAE;MACnBC,UAAU,EAAE,EAAE;MACdC,YAAY,EAAE,IAAI;MAClBC,QAAQ,EAAE,EAAE;MACZC,UAAU,EAAE,KAAK;MACjBC,MAAM,EAAE;IACV,CAAC;EACH,CAAC;EACDC,OAAO,EAAE;IACPC,YAAYA,CAACC,KAAK,EAAE;MAClB,IAAI,CAACN,YAAW,GAAIM,KAAK,CAACC,MAAM,CAACC,KAAK,CAAC,CAAC,CAAC;IAC3C,CAAC;IACD,MAAMC,UAAUA,CAAA,EAAG;MACjB,IAAI,CAAC,IAAI,CAACT,YAAY,EAAE,OAAO,IAAI,CAACC,QAAQ;MAE5C,MAAMS,QAAO,GAAI,IAAIC,QAAQ,CAAC,CAAC;MAC/BD,QAAQ,CAACE,MAAM,CAAC,MAAM,EAAE,IAAI,CAACZ,YAAY,CAAC;MAE1C,IAAI;QACF,MAAMa,QAAO,GAAI,MAAMpB,KAAK,CAACqB,IAAI,CA