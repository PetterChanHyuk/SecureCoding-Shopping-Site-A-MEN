       formData.append('file', this.selectedFile);\r\n  \r\n        try {\r\n          const response = await axios.post(`${process.env.VUE_APP_BACKEND_URL}/upload`, formData, {\r\n            headers: {\r\n              'Content-Type': 'multipart/form-data'\r\n            }\r\n          });\r\n          return response.data.filePath;\r\n        } catch (error) {\r\n          console.error('파일 업로드 실패:', error);\r\n          return '';\r\n        }\r\n      },\r\n      async addItem() {\r\n        if (!this.itemName || !this.itemCategory) {\r\n          alert('아이템 이름과 카테고리를 선택해주세요.');\r\n          return;\r\n        }\r\n  \r\n        const imageUrl = await this.uploadFile();\r\n  \r\n        const newItem = {\r\n          name: this.itemName,\r\n          categoryId: this.itemCategory,\r\n          imageUrl,\r\n          description: this.itemDescription\r\n        };\r\n  \r\n        axios.post(`${process.env.VUE_APP_BACKEND_URL}/items`, newItem)\r\n          .then(() => {\r\n            alert('아이템이 성공적으로 추가되었습니다.');\r\n            this.resetForm();\r\n          })\r\n          .catch(error => {\r\n            console.error('아이템 추가 실패:', error);\r\n          });\r\n      },\r\n      async updateItem() {\r\n        if (!this.itemName || !this.itemCategory) {\r\n          alert('아이템 이름과 카테고리를 선택해주세요.');\r\n          return;\r\n        }\r\n  \r\n        const imageUrl = await this.uploadFile();\r\n  \r\n        const updatedItem = {\r\n          name: this.itemName,\r\n          categoryId: this.itemCategory,\r\n          imageUrl,\r\n          description: this.itemDescription\r\n        };\r\n  \r\n        axios.put(`${process.env.VUE_APP_BACKEND_URL}/items/${this.itemId}`, updatedItem)\r\n          .then(() => {\r\n            alert('아이템이 성공적으로 수정되었습니다.');\r\n            this.$router.push('/');\r\n          })\r\n          .catch(error => {\r\n            console.error('아이템 수정 실패:', error);\r\n          });\r\n      },\r\n      fetchItemDetails() {\r\n        const itemId = this.$route.params.id;\r\n        if (itemId) {\r\n          this.isEditMode = true;\r\n          axios.get(`${process.env.VUE_APP_BACKEND_URL}/items/${itemId}`)\r\n            .then(response => {\r\n              const item = response.data;\r\n              this.itemId = item.id;\r\n              this.itemName = item.name;\r\n              this.itemCategory = item.category_id;\r\n              this.itemDescription = item.description;\r\n              this.imageUrl = item.image_url;\r\n            })\r\n            .catch(error => {\r\n              console.error('Error fetching item details:', error);\r\n            });\r\n        }\r\n      },\r\n      fetchCategories() {\r\n        axios.get(`${process.env.VUE_APP_BACKEND_URL}/categories`)\r\n          .then(response => {\r\n            this.categories = response.data;\r\n          })\r\n          .catch(error => {\r\n            console.error('Error fetching categories:', error);\r\n          });\r\n      },\r\n      resetForm() {\r\n        this.itemName = '';\r\n        this.itemCategory = null;\r\n        this.itemDescription = '';\r\n        this.selectedFile = null;\r\n        this.imageUrl = '';\r\n        this.isEditMode = false;\r\n        this.itemId = null;\r\n      }\r\n    },\r\n    created() {\r\n      this.fetchCategories();\r\n      this.fetchItemDetails();\r\n    }\r\n  };\r\n  </script>\r\n  \r\n  <style>\r\n  .add-item-page {\r\n    margin-top: 20px;\r\n    padding: 20px;\r\n    border: 1px solid #ddd;\r\n    display: flex;\r\n    flex-direction: column;\r\n    align-items: center;\r\n  }\r\n  .add-item-page input,\r\n  .add-item-page select,\r\n  .add-item-page textarea {\r\n    padding: 10px;\r\n    font-size: 16px;\r\n    margin-bottom: 10px;\r\n  }\r\n  .add-item-page button {\r\n    padding: 10px 20px;\r\n    font-size: 16px;\r\n    cursor: pointer;\r\n  }\r\n  </style>\r\n  "],"mappings":";;EACSA,KAAK,EAAC;AAAe;mBAD9B;;uBACIC,mBAAA,CAWM,OAXNC,UAWM,GAVJC,mBAAA,CAAmD,YAAAC,gBAAA,