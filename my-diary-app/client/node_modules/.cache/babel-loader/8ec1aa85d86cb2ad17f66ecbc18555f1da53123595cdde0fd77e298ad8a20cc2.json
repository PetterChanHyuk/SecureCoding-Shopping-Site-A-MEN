{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport axios from 'axios';\nexport default {\n  data() {\n    return {\n      userName: '익명',\n      orders: []\n    };\n  },\n  methods: {\n    async fetchOrders() {\n      const userId = localStorage.getItem('userId');\n      try {\n        const response = await axios.get(`${process.env.VUE_APP_BACKEND_URL}/orders/${userId}`);\n        this.orders = await Promise.all(response.data.map(async order => {\n          const itemsResponse = await axios.get(`${process.env.VUE_APP_BACKEND_URL}/order/${order.id}`);\n          return {\n            ...order,\n            items: itemsResponse.data\n          };\n        }));\n      } catch (error) {\n        console.error('Failed to fetch orders:', error);\n      }\n    },\n    async deleteOrder(orderId) {\n      try {\n        await axios.delete(`${process.env.VUE_APP_BACKEND_URL}/orders/${orderId}`);\n        this.orders = this.orders.filter(order => order.id !== orderId);\n        alert('주문이 삭제되었습니다.');\n      } catch (error) {\n        console.error('Failed to delete order:', error);\n        alert('주문 삭제에 실패했습니다.');\n      }\n    },\n    async fetchUserName() {\n      try {\n        const response = await axios.get(`${process.env.VUE_APP_BACKEND_URL}/username`, {\n          params: {\n            userId: localStorage.getItem('userId')\n          }\n        });\n        this.userName = response.data.name;\n      } catch (error) {\n        console.error('Error fetching user name:', error);\n      }\n    },\n    goToMainPage() {\n      this.$router.push('/mainpage');\n    },\n    async logout() {\n      const userId = localStorage.getItem('userId');\n      try {\n        await axios.post(`${process.env.VUE_APP_BACKEND_URL}/userlogout`, {\n          userId\n        });\n        localStorage.removeItem('userId');\n        this.$router.push('/userlogin');\n      } catch (error) {\n        console.error('로그아웃 실패:', error);\n      }\n    }\n  },\n  created() {\n    this.fetchUserName();\n    this.fetchOrders();\n    const userId = localStorage.getItem('userId');\n    if (!userId) {\n      alert(\"비정상적인 접근입니다.\");\n      this.$router.push('/userlogin');\n    }\n  }\n};","map":{"version":3,"names":["axios","data","userName","orders","methods","fetchOrders","userId","localStorage","getItem","response","get","process","env","VUE_APP_BACKEND_URL","Promise","all","map","order","itemsResponse","id","items","error","console","deleteOrder","orderId","delete","filter","alert","fetchUserName","params","name","goToMainPage","$router","push","logout","post","removeItem","created"],"sources":["D:\\SecureCoding-Shopping-Site-A-MEN\\my-diary-app\\client\\src\\components\\OrderHistoryPage.vue"],"sourcesContent":["<template>\r\n    <div class=\"order-history-container\">\r\n      <header>\r\n        <h1 @click=\"goToMainPage\">108번가</h1>\r\n        <div class=\"user-info\">\r\n          {{ userName }}님 | \r\n          <router-link to=\"/mypage\">마이페이지</router-link> |\r\n          <a href=\"#\" @click=\"logout\">로그아웃</a>\r\n        </div>\r\n      </header>\r\n      <main>\r\n        <h2>구매 목록</h2>\r\n        <div v-if=\"orders.length === 0\">구매 목록이 없습니다.</div>\r\n        <div v-else>\r\n          <div v-for=\"order in orders\" :key=\"order.id\" class=\"order-item\">\r\n            <h3>주문 ID: {{ order.id }}</h3>\r\n            <div v-for=\"item in order.items\" :key=\"item.item_id\" class=\"order-item-details\">\r\n              <p>{{ item.name }} - {{ item.quantity }}개 - {{ item.price }}원</p>\r\n            </div>\r\n            <p>총 금액: {{ order.total_amount }}원</p>\r\n            <button @click=\"deleteOrder(order.id)\">삭제</button>\r\n          </div>\r\n        </div>\r\n      </main>\r\n    </div>\r\n  </template>\r\n  \r\n  <script>\r\n  import axios from 'axios';\r\n  \r\n  export default {\r\n    data() {\r\n      return {\r\n        userName: '익명',\r\n        orders: []\r\n      };\r\n    },\r\n    methods: {\r\n      async fetchOrders() {\r\n        const userId = localStorage.getItem('userId');\r\n        try {\r\n          const response = await axios.get(`${process.env.VUE_APP_BACKEND_URL}/orders/${userId}`);\r\n          this.orders = await Promise.all(response.data.map(async order => {\r\n            const itemsResponse = await axios.get(`${process.env.VUE_APP_BACKEND_URL}/order/${order.id}`);\r\n            return { ...order, items: itemsResponse.data };\r\n          }));\r\n        } catch (error) {\r\n          console.error('Failed to fetch orders:', error);\r\n        }\r\n      },\r\n      async deleteOrder(orderId) {\r\n        try {\r\n          await axios.delete(`${process.env.VUE_APP_BACKEND_URL}/orders/${orderId}`);\r\n          this.orders = this.orders.filter(order => order.id !== orderId);\r\n          alert('주문이 삭제되었습니다.');\r\n        } catch (error) {\r\n          console.error('Failed to delete order:', error);\r\n          alert('주문 삭제에 실패했습니다.');\r\n        }\r\n      },\r\n      async fetchUserName() {\r\n        try {\r\n          const response = await axios.get(`${process.env.VUE_APP_BACKEND_URL}/username`, {\r\n            params: { userId: localStorage.getItem('userId') }\r\n          });\r\n          this.userName = response.data.name;\r\n        } catch (error) {\r\n          console.error('Error fetching user name:', error);\r\n        }\r\n      },\r\n      goToMainPage() {\r\n        this.$router.push('/mainpage');\r\n      },\r\n      async logout() {\r\n        const userId = localStorage.getItem('userId');\r\n        try {\r\n          await axios.post(`${process.env.VUE_APP_BACKEND_URL}/userlogout`, { userId });\r\n          localStorage.removeItem('userId');\r\n          this.$router.push('/userlogin');\r\n        } catch (error) {\r\n          console.error('로그아웃 실패:', error);\r\n        }\r\n      }\r\n    },\r\n    created() {\r\n      this.fetchUserName();\r\n      this.fetchOrders();\r\n      \r\n      const userId = localStorage.getItem('userId');\r\n      if (!userId) {\r\n        alert(\"비정상적인 접근입니다.\");\r\n        this.$router.push('/userlogin');\r\n      }\r\n    }\r\n  };\r\n  </script>\r\n  \r\n  <style>\r\n  .order-history-container {\r\n    text-align: center;\r\n  }\r\n  header {\r\n    background-color: #f8f9fa;\r\n    padding: 20px;\r\n    display: flex;\r\n    justify-content: space-between;\r\n    align-items: center;\r\n  }\r\n  h1 {\r\n    margin: 0;\r\n    cursor: pointer;\r\n  }\r\n  main {\r\n    padding: 20px;\r\n  }\r\n  .order-item {\r\n    border: 1px solid #dee2e6;\r\n    padding: 10px;\r\n    margin: 10px 0;\r\n  }\r\n  .order-item-details {\r\n    padding-left: 20px;\r\n  }\r\n  button {\r\n    padding: 10px 20px;\r\n    font-size: 16px;\r\n    cursor: pointer;\r\n    background-color: #007bff;\r\n    color: white;\r\n    border: none;\r\n    border-radius: 5px;\r\n    margin: 10px 0;\r\n  }\r\n  button:hover {\r\n    background-color: #0056b3;\r\n  }\r\n  </style>\r\n  "],"mappings":";AA4BE,OAAOA,KAAI,MAAO,OAAO;AAEzB,eAAe;EACbC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,QAAQ,EAAE,IAAI;MACdC,MAAM,EAAE;IACV,CAAC;EACH,CAAC;EACDC,OAAO,EAAE;IACP,MAAMC,WAAWA,CAAA,EAAG;MAClB,MAAMC,MAAK,GAAIC,YAAY,CAACC,OAAO,CAAC,QAAQ,CAAC;MAC7C,IAAI;QACF,MAAMC,QAAO,GAAI,MAAMT,KAAK,CAACU,GAAG,CAAE,GAAEC,OAAO,CAACC,GAAG,CAACC,mBAAoB,WAAUP,MAAO,EAAC,CAAC;QACvF,IAAI,CAACH,MAAK,GAAI,MAAMW,OAAO,CAACC,GAAG,CAACN,QAAQ,CAACR,IAAI,CAACe,GAAG,CAAC,MAAMC,KAAI,IAAK;UAC/D,MAAMC,aAAY,GAAI,MAAMlB,KAAK,CAACU,GAAG,CAAE,GAAEC,OAAO,CAACC,GAAG,CAACC,mBAAoB,UAASI,KAAK,CAACE,EAAG,EAAC,CAAC;UAC7F,OAAO;YAAE,GAAGF,KAAK;YAAEG,KAAK,EAAEF,aAAa,CAACjB;UAAK,CAAC;QAChD,CAAC,CAAC,CAAC;MACL,EAAE,OAAOoB,KAAK,EAAE;QACdC,OAAO,CAACD,KAAK,CAAC,yBAAyB,EAAEA,KAAK,CAAC;MACjD;IACF,CAAC;IACD,MAAME,WAAWA,CAACC,OAAO,EAAE;MACzB,IAAI;QACF,MAAMxB,KAAK,CAACyB,MAAM,CAAE,GAAEd,OAAO,CAACC,GAAG,CAACC,mBAAoB,WAAUW,OAAQ,EAAC,CAAC;QAC1E,IAAI,CAACrB,MAAK,GAAI,IAAI,CAACA,MAAM,CAACuB,MAAM,CAACT,KAAI,IAAKA,KAAK,CAACE,EAAC,KAAMK,OAAO,CAAC;QAC/DG,KAAK,CAAC,cAAc,CAAC;MACvB,EAAE,O