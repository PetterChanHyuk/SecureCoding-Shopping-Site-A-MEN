gory: null,\r\n        itemDescription: '',\r\n        categories: [],\r\n        selectedFile: null,\r\n        imageUrl: '',\r\n        itemLoaded: false\r\n      };\r\n    },\r\n    methods: {\r\n      onFileChange(event) {\r\n        this.selectedFile = event.target.files[0];\r\n      },\r\n      async uploadFile() {\r\n        if (!this.selectedFile) return this.imageUrl;\r\n  \r\n        const formData = new FormData();\r\n        formData.append('file', this.selectedFile);\r\n  \r\n        try {\r\n          const response = await axios.post(`${process.env.VUE_APP_BACKEND_URL}/upload`, formData, {\r\n            headers: {\r\n              'Content-Type': 'multipart/form-data'\r\n            }\r\n          });\r\n          return response.data.filePath;\r\n        } catch (error) {\r\n          console.error('파일 업로드 실패:', error);\r\n          return '';\r\n        }\r\n      },\r\n      async saveItem() {\r\n        if (!this.itemName || !this.itemCategory) {\r\n          alert('아이템 이름과 카테고리를 선택해주세요.');\r\n          return;\r\n        }\r\n  \r\n        const imageUrl = await this.uploadFile();\r\n  \r\n        const itemData = {\r\n          name: this.itemName,\r\n          categoryId: this.itemCategory,\r\n          imageUrl,\r\n          description: this.itemDescription\r\n        };\r\n  \r\n        axios.put(`${process.env.VUE_APP_BACKEND_URL}/items/${this.itemId}`, itemData)\r\n          .then(() => {\r\n            alert('아이템이 성공적으로 수정되었습니다.');\r\n            this.$router.push('/');\r\n          })\r\n          .catch(error => {\r\n            console.error('아이템 수정 실패:', error);\r\n          });\r\n      },\r\n      fetchItemDetails() {\r\n        const itemId = this.$route.params.id;\r\n        if (itemId) {\r\n          axios.get(`${process.env.VUE_APP_BACKEND_URL}/items/${itemId}`)\r\n            .then(response => {\r\n              const item = response.data;\r\n              this.itemId = item.id;\r\n              this.itemName = item.name;\r\n              this.itemCategory = item.category_id;\r\n              this.itemDescription = item.description;\r\n              this.imageUrl = item.image_url;\r\n              this.itemLoaded = true; // 데이터 로드 완료\r\n            })\r\n            .catch(error => {\r\n              console.error('Error fetching item details:', error);\r\n              alert('아이템 데이터를 불러오는데 실패했습니다.');\r\n            });\r\n        } else {\r\n          alert('아이템 ID가 제공되지 않았습니다.');\r\n          this.$router.push('/');\r\n        }\r\n      },\r\n      fetchCategories() {\r\n        axios.get(`${process.env.VUE_APP_BACKEND_URL}/categories`)\r\n          .then(response => {\r\n            this.categories = response.data;\r\n          })\r\n          .catch(error => {\r\n            console.error('Error fetching categories:', error);\r\n          });\r\n      }\r\n    },\r\n    created() {\r\n      this.fetchCategories();\r\n      this.fetchItemDetails();\r\n    }\r\n  };\r\n  </script>\r\n  \r\n  <style>\r\n  .edit-item-page {\r\n    margin-top: 20px;\r\n    padding: 20px;\r\n    border: 1px solid #ddd;\r\n    display: flex;\r\n    flex-direction: column;\r\n    align-items: center;\r\n  }\r\n  .edit-item-page input,\r\n  .edit-item-page select,\r\n  .edit-item-page textarea {\r\n    padding: 10px;\r\n    font-size: 16px;\r\n    margin-bottom: 10px;\r\n  }\r\n  .edit-item-page button {\r\n    padding: 10px 20px;\r\n    font-size: 16px;\r\n    cursor: pointer;\r\n  }\r\n  </style>\r\n  "],"mappings":";;EACSA,KAAK,EAAC;AAAgB;gCACzBC,mBAAA,CAAe,YAAX,QAAM;;EAFhBC,GAAA;AAAA;mBAAA;;EAAAA,GAAA;AAAA;gCAeQD,mBAAA,CAAyB,WAAtB,oBAAkB;oBAArBE,UAAyB,C;;uBAd7BC,mBAAA,CAgBM,OAhBNC,UAgBM,GAfJC,UAAe,EACJC,KAAA,CAAAC,UAAU,I,cAArBJ,mBAAA,CAUM,OAbZK,UAAA,G,gBAIQR,mBAAA,CAAgE;IAAzDS,IAAI,EAAC,MAAM;IAJ1B,uBAAAC,MAAA,QAAAA,MAAA,MAAAC,MAAA,IAIoCL,KAAA,CAAAM,QAAQ,GAAAD,MAAA;IAAEE,WAAW,EAAC;iDAAtBP,KAAA,CAAAM,QAAQ,E,mBACpCZ,mBAAA,CAIS;IATjB,uBAAAU,MAAA,QAAAA,MAAA,MAAAC,MAAA,IAKyBL,KAAA,CAAAQ,YAAY,GAAAH,MAAA;yBAC3B